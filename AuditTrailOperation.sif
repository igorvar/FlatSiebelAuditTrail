	{
		bcItoAT.NewRecord(NewAfter);
		bcItoAT.SetFieldValue(&quot;Record Id&quot;, 						bcSiebelAT.GetFieldValue(&quot;Record Id&quot;));
		bcItoAT.SetFieldValue(&quot;Audit Source&quot;,					bcSiebelAT.GetFieldValue(&quot;Audit Source&quot;));
