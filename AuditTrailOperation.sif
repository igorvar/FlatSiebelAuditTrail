			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="Refresh"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function Refresh(bcName, Id)
{
	var thisFunctionName = &quot;Refresh&quot;;
	var bcSiebelAT: BusComp;
	var bcItoAT:BusComp;
	var childRecords = new Array();
	try
	{
		RemovePresentsRecords(bcName, Id);
		bcSiebelAT = TheApplication().GetBusObject(&quot;Audit Trail&quot;).GetBusComp(&quot;Audit Trail Item 2&quot;);
		bcSiebelAT.SetSearchSpec(&quot;Record Id&quot;, Id);
		bcSiebelAT.SetSearchSpec(&quot;Business Component&quot;, bcName);
		bcSiebelAT.ActivateField(&quot;Record Id&quot;);
		bcSiebelAT.ActivateField(&quot;Audit Source&quot;);
		bcSiebelAT.ActivateField(&quot;Buscomp Base Table&quot;);
		bcSiebelAT.ActivateField(&quot;Business Component&quot;);
		bcSiebelAT.ActivateField(&quot;Child Buscomp Base Table&quot;);
		bcSiebelAT.ActivateField(&quot;Child Buscomp Record Id&quot;);
		bcSiebelAT.ActivateField(&quot;Child Business Component&quot;);
		bcSiebelAT.ActivateField(&quot;Column&quot;);
		bcSiebelAT.ActivateField(&quot;Date&quot;);
		bcSiebelAT.ActivateField(&quot;Date Synchronized&quot;);
		bcSiebelAT.ActivateField(&quot;Employee Id&quot;);
		bcSiebelAT.ActivateField(&quot;Field&quot;);
		bcSiebelAT.ActivateField(&quot;Group Id&quot;);
		bcSiebelAT.ActivateField(&quot;Link Name&quot;);
		bcSiebelAT.ActivateField(&quot;New Value&quot;);
		bcSiebelAT.ActivateField(&quot;Node&quot;);
		bcSiebelAT.ActivateField(&quot;Old Value&quot;);
		bcSiebelAT.ActivateField(&quot;Operation&quot;);
		bcSiebelAT.ActivateField(&quot;Record Id&quot;);
		bcSiebelAT.ActivateField(&quot;Table Name&quot;);
		bcSiebelAT.ActivateField(&quot;Table Row Id&quot;);
		bcSiebelAT.ExecuteQuery(ForwardOnly);
		
		if(!bcSiebelAT.FirstRecord()) return; // no record in standart bc 
		
		bcItoAT = TheApplication().GetBusObject(&quot;Audit Trail&quot;).GetBusComp(&quot;VIF Audit Trail Item Flat&quot;);
		do
		{
			CopyRecord(bcSiebelAT, bcItoAT, bcName, Id);
	//		if(bcSiebelAT.GetFieldValue(&quot;Child Buscomp Record Id&quot;) != &quot;&quot;) If row added and after this removed will by will double recording about update of child
			if (bcSiebelAT.GetFieldValue(&quot;Operation&quot;) == &quot;Associate&quot;)
				childRecords[childRecords.length] = &quot;[Business Component] = &apos;&quot; + bcSiebelAT.GetFieldValue(&quot;Child Business Component&quot;) + &quot;&apos; and [Record Id] = &apos;&quot; + bcSiebelAT.GetFieldValue(&quot;Child Buscomp Record Id&quot;) + &quot;&apos;&quot;
		}while (bcSiebelAT.NextRecord());
	
		//for Child records
		for (var i = 0; i &lt; childRecords.length; i++)
		{
			bcSiebelAT.ClearToQuery();
			bcSiebelAT.SetSearchExpr(childRecords[i]);
			bcSiebelAT.ExecuteQuery(ForwardOnly);
			if(!bcSiebelAT.FirstRecord()) continue; // to begin FOR loop
			do
			{
				CopyRecord(bcSiebelAT, bcItoAT, bcName, Id);
				/* 
				This block for grandchildren, grandgrandchildren... If record of children contain reference to his own child, 
				array of searchspeck increated
				not tested
				if (bcSiebelAT.GetFieldValue(&quot;Operation&quot;) == &quot;Associate&quot;)
					childRecords[childRecords.length] = &quot;[Business Component] = &apos;&quot; + bcSiebelAT.GetFieldValue(&quot;Child Business Component&quot;) + &quot;&apos; and [Record Id] = &apos;&quot; + bcSiebelAT.GetFieldValue(&quot;Child Buscomp Record Id&quot;) + &quot;&apos;&quot;
				*/
			}while (bcSiebelAT.NextRecord());
		}
	} // end of try{}
	catch(e)
	{
		WriteToLog(thisFunctionName, e.message);
		TheApplication().RaiseErrorText(e.message);
	}
	finally
	{
		bcSiebelAT = null;
		bcItoAT = null;
		childRecords = null;
	}
}"
			   SEQUENCE="2">
			</BUSINESS_SERVICE_SERVER_SCRIPT>
			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="RemovePresentsRecords"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function RemovePresentsRecords(rootBcName, rootRecordId)
{
	var thisFunctionName = &quot;RemovePresentsRecords&quot;;
	var bc:BusComp;
	try
	{
		bc = TheApplication().GetBusObject(&quot;Audit Trail&quot;).GetBusComp(&quot;VIF Audit Trail Item Flat&quot;);
		bc.SetSearchSpec(&quot;VIF Root Business Component&quot;, rootBcName);
		bc.SetSearchSpec(&quot;VIF Root Record Id&quot;, rootRecordId);
		bc.ExecuteQuery(ForwardOnly);
	
		if (!bc.FirstRecord()) return;
	
		do
		{
			bc.DeleteRecord();
		} while(bc.FirstRecord());
	}
	catch(e)
	{
		WriteToLog(thisFunctionName, e.message);
		TheApplication().RaiseErrorText(e.message);
	}
	finally
	{
		bc = null;
	}
}"
			   SEQUENCE="3">
			</BUSINESS_SERVICE_SERVER_SCRIPT>
